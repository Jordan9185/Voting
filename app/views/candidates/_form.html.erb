<%= form_for @Candidate do |f| %>
	<%=f.label :name, "Name"%>
	<%=f.text_field :name %> <br>

	<%=f.label :age, "Age"%>
	<%=f.text_field :age %> <br>

	<%=f.label :party, "Party"%>
	<%=f.text_field :party %> <br>

	<%=f.label :politics, "Politics"%>
	<%=f.text_field :politics %> <br>

	<%=f.submit %>
<% end %>

<br>